# Video Processing Configuration
# Frame sampling rate - process every Nth frame from video
FRAME_SAMPLE_RATE=1

# HLoC NetVLAD Retrieval Configuration
# Use NetVLAD retrieval instead of exhaustive matching for large datasets
USE_NETVLAD_RETRIEVAL=True
# Number of most similar images to match per image (5-50 typical, 20 recommended)
NETVLAD_NUM_MATCHED=20